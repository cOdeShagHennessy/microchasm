/**
* <%= userName %> generated on <% var date = new Date()%><%=date.toLocaleString()%> using slush-mChasm.
* Author:<%= authorName %>
* Email:<%= authorEmail %>
* Base topology configuration. Only need to supply overrides for values being changed for specific environments
* being supported.
*/
module.exports.Config = function (environment) {
    return {
        filter: environment, <% _.forEach(fields, function(field) { %>
        <%=field.name%>: <%switch (field.type) { case 'string': %>"<%=field.defValue%>",<%break;case 'date': %>Date.now(),<%break; default: %><%=field.defValue%>, <%}%> <% }); %>
    }
};
